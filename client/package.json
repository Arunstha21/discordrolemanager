{
  "name": "discordrolemanager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:next": "next build",
    "build": "npm run build-backend && npm run build-frontend",
    "start:server": "cd server && npm start",
    "start": "concurrently \"npm run start:server\" \"next start\"",
    "build-frontend": "npm install && npm run build:next",
    "build-backend": "cd server && npm install"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "discord.js": "^14.15.3",
    "lucide-react": "^0.414.0",
    "mongoose": "^8.5.1",
    "next": "14.2.5",
    "papaparse": "^5.4.1",
    "radix-ui": "^1.0.1",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.4.0",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1"
  }
}
